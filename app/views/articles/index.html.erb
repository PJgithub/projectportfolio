<div class="page-header">
<h1>Listing articles</h1>
</div>

<%= link_to "Create a new project", new_article_path, class: "btn btn-success" %>

<% @articles.each do |article| %> 
<div class="article"> 
  <strong><%= article.name %></strong> 
  <p><%= article.description %></p> 
  <p><%= article.image %></p> 
  <div class="meta"> 
    <%= link_to time_ago_in_words(article.created_at) +" ago", article %> | 
    <span class="admin"> 
      <%= link_to "Edit", edit_article_path(article) %> | 
      <%= link_to "Delete", article, method: :delete, data: { confirm: "Are you sure you want to delete this article?"} %>
    </span>
    </div>
  </div>
<% end %>



















